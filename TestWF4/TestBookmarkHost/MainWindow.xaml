<Window x:Class="TestBookmarkHost.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="286" Width="390">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="40" />
            <RowDefinition Height="40" />
            <RowDefinition Height="40" />
            <RowDefinition Height="123*" />
        </Grid.RowDefinitions>
        <Button Content="NoResult" Height="23" HorizontalAlignment="Center" Name="button1" Width="160" Click="button1_Click" Grid.Row="1" />
        <Button Content="Result" Height="23" HorizontalAlignment="Center" Name="button2" Width="160" Click="button2_Click" Grid.Row="2" />
    </Grid>
</Window>
